{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["./src"],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "\\.jsx"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": false
		}
	},
	"opts": {
		"destination": "docs",
		"recurse": true,
		"readme": "README.md",
		"explain": true
	}
}
